<% controller_path = controller.class.name.to_snake[0..-12] %>
<% if lookup_context.find_all("#{controller_path}/_actions").any? %>
	<% partial = render(partial: "#{controller_path}/actions", locals: { options: RuthApplication.actions_options }) %>
	<% if !strip_tags(partial).trim(" \t\n").blank? %>
		<%= partial %>
	<% end %>
<% end %>